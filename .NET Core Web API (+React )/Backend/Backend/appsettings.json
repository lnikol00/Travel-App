{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": " Data Source=DESKTOP-0VO0VSO\\SQLEXPRESS;Database=TravelDB;TrustServerCertificate=True;Integrated Security=True"
